# git stuff
alias gcm='git bulk checkout master'
alias gbs='git bulk status'
alias gs='git status'
alias gitmerge='git mergetool -t vimdiff'

alias  gam='git commit -am'
alias  grm='git rm'
alias  gmv='git mv'
alias  grr='git reset --hard head'
alias  goop="git commit --amend -c head -n"
alias  ga='git add'
alias  gd='git diff | subl'
alias  gp='git push'
alias  gl='git log --name-status'
alias  gf='git fetch -p'
alias  gm='git commit -m'
alias  gb='git branch'
alias  gc='git checkout'
alias  gff="git fetch -p && git rebase origin/\$(git_prompt_info)"
# vim: set syn=sh :
